class Solution {
public:
    int minStartValue(vector<int>& nums) {
        int sum=0;
        int minsum=0;
      for(int i=0;i<nums.size();i++){
        sum+=nums[i];
        if(sum<=minsum){
          minsum=sum;
        }
      }

    //   let minsum+x>=1  ==>find x ?
    // then x=1-minsum
      return 1-minsum;
    }
};
