class Solution {
public:
    int partitionDisjoint(vector<int>& nums) {
        int n=nums.size();
        int leftmax=nums[0];
        int sabseBada= nums[0]; //travarse each element in an array
        int partition=0; //store indxe from left side
        for(int i=0;i<n;i++){
            sabseBada=max(sabseBada,nums[i]);
            if(nums[i]<leftmax){
                partition=i;
                leftmax=sabseBada;
            }
        }
        return 1+partition; 
    }
};
